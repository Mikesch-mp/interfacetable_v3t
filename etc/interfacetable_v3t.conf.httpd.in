ScriptAlias @cgiurlrel@ "@cgidir@"

<Directory "@cgidir@">
    Options ExecCGI
    AllowOverride None
    Order allow,deny
    Allow from all
    AuthName "@apache_authname@"
    AuthType Basic
    AuthUserFile @nagios_etc@/htpasswd.users
    Require valid-user
</Directory>

Alias @htmlurlrel@ "@sharedir@"

<Directory "@sharedir@">
    Options None
    AllowOverride None
    Order allow,deny
    Allow from all
    AuthName "@apache_authname@"
    AuthType Basic
    AuthUserFile @nagios_etc@/htpasswd.users
    Require valid-user
</Directory>
